<Window x:Class="Semana2Rudas.Reportes"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Semana2Rudas"
        mc:Ignorable="d"
        Title="Reportes" Height="450" Width="800">
    <Grid>
        <StackPanel Margin="20">

            <Button Content="Cargas" Click="Button_Cargas" Margin="0,30,0,0"/>
            <Button Content="Ingresos" Click="Button_Ingresos" Margin="0,30,0,0"/>
            <Button Content="Salidas" Click="Button_Salidas" Margin="0,30,0,0"/>

        </StackPanel>
    </Grid>
</Window>
